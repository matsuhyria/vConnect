						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"const baseUrl = pm.collectionVariables.get(\"base_url\");",
									"const version = pm.collectionVariables.get(\"version\");",
									"const endpoint = 'users';",
									"",
									"const url = `${baseUrl}/api/${version}/${endpoint}`;",
									"",
									"// Define the signup request details",
									"const signupRequest = {",
									"    url,",
									"    method: 'POST',",
									"    header: {",
									"        'Content-Type': 'application/json'",
									"    },",
									"    body: {",
									"        mode: 'raw',",
									"        raw: JSON.stringify({",
									"            name: \"deleted\",",
									"            email: 'deleted@dxyz.com',",
									"            password: '123'",
									"        })",
									"    }",
									"};",
									"",
									"pm.sendRequest(signupRequest, function (err, res) {",
									"    if (err) {",
									"        console.error('Signup request failed:', err);",
									"        return;",
									"    }",
									"",
									"    var jsonData = res.json();",
									"    pm.environment.set(\"user_id\", jsonData.user.id);",
									"    pm.environment.set(\"token\", jsonData.token);",
									"});",
									"",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
